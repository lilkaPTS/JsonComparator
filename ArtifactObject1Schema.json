{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "properties": {
    "mvn": {
      "minItems": 1,
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "artifactId": {
            "type": "string"
          },
          "classifier": {
            "type": "string"
          },
          "groupId": {
            "type": "string"
          },
          "hashes": {
            "type": "object",
            "properties": {
              "sha1": {
                "type": "string"
              },
              "sha256": {
                "type": "string"
              }
            },
            "required": [
              "sha1",
              "sha256"
            ]
          },
          "mvn_repository": {
            "type": "string"
          },
          "mvn_type": {
            "type": "string"
          },
          "service_name": {
            "type": "string"
          },
          "version": {
            "type": "string"
          }
        },
        "required": [
          "artifactId",
          "groupId",
          "hashes",
          "mvn_repository",
          "mvn_type",
          "version"
        ]
      }
    },
    "target_repository": {
      "type": "string"
    }
  },
  "required": [
    "mvn",
    "target_repository"
  ]
}
